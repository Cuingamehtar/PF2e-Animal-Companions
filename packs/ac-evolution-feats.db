{"_id":"rhWFwue5eYVr5Omr","name":"Shrink Down","type":"feat","img":"systems/pf2e/icons/features/feats/feats.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.feats-srd.RonS3ZJs4poFTckH"}},"system":{"description":{"value":"<p>Your eidolon can shrink to fit into small spaces. Your eidolon reduces in size, to a minimum of size Small. It can use this action again while shrunk to return to its normal size.</p>"},"source":{"value":""},"traits":{"value":["concentrate","eidolon","evolution","summoner","transmutation"],"rarity":"common","custom":""},"rules":[],"slug":"shrink-down","schema":{"version":0.788,"lastMigration":{"datetime":"2022-09-17T22:42:48.395-04:00","version":{"schema":0.713,"foundry":"10.285","system":"4.0.6"}}},"level":{"value":4},"featType":{"value":"class"},"onlyLevel1":false,"maxTakable":1,"actionType":{"value":"action"},"actionCategory":{"value":"interaction"},"actions":{"value":1},"prerequisites":{"value":[]},"location":null},"ownership":{"default":0,"0tWA9mscoVx3YLRI":3},"_stats":{"systemId":"pf2e","systemVersion":"4.0.6","coreVersion":"10.285","createdTime":1663468968463,"modifiedTime":1663469038376,"lastModifiedBy":"thw0Ox3kKEThnAFP"}}
